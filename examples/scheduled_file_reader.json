{
    "flow": {
        "name:": "file_reader",
        "tasks": [
            {
                "generate": {
                    "label": "run_sample_reader",
                    "interval": 1,
                    "count": 1,
                    "message": "{\"key\":\"value\"}"
                }
            },
            {
                "file_reader": {
                    "path": "/tmp/data/sample.in.csv"
                }
            },
            {
                "enumerate": {
                    "label": "sample.in",
                    "array": {
                        "column": "payload",
                        "is_static": false,
                        "is_extension": false
                    }
                }
            },
            {
                "file_writer": {
                    "path": "/tmp/data/sample.out.csv"
                }
            }
        ]
    }
}